{
  "name": "p5-react-renderer",
  "version": "0.0.5",
  "description": "React-fiber renderer for p5.js",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "jsnext:main": "dist/parallax-provider.esm.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adiman9/p5-react-renderer.git"
  },
  "author": "Adrian Henry <adrian@hungryturtlecode.com>",
  "license": "MIT",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint src",
    "clean": "rimraf dist",
    "precommit": "lint-staged",
    "format": "prettier --write \"src/**/*.+(js|md|ts|css|sass|less|graphql|yml|yaml|scss|json|vue)\"",
    "test": "jest",
    "test:watch": "jest src --watch --notify",
    "prepublishOnly": "npm run clean && npm run lint && npm run test && npm run build"
  },
  "lint-staged": {
    "**/*.+(js|md|ts|css|sass|less|graphql|yml|yaml|scss|json|vue)": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "prop-types": "^15.7.2",
    "react-reconciler": "^0.20.2",
    "scheduler": "^0.13.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "eslint": "^5.15.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.5.0",
    "lint-staged": "^8.1.5",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "rollup": "^1.5.0",
    "rollup-plugin-babel": "^4.3.2"
  },
  "peerDependencies": {
    "p5": "^0.7.3",
    "react": ">=16.8",
    "react-dom": ">=16.8"
  },
  "keywords": [
    "react",
    "renderer",
    "fiber",
    "p5",
    "p5js",
    "p5.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "bugs": {
    "url": "https://github.com/adiman9/p5-react-renderer/issues"
  },
  "homepage": "https://github.com/adiman9/p5-react-renderer#readme"
}
